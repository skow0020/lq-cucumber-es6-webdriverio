{
  "name": "cucumberjs-webdriverio-ui-automation",
  "version": "1.0.0",
  "description": "CucumberJS WebdriverIO ES6 ui automation",
  "author": "Colin Skow <skow0020@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "config": {
    "host": "localhost",
    "port": "4444"
  },
  "scripts": {
    "gherkin-lint": "gherkin-lint",
    "lint": "eslint .",
    "dev": "wdio wdio.dev.conf.js",
    "test": "wdio wdio.test.conf.js",
    "jenkins": "wdio wdio.conf.js --host $npm_package_config_host"
  },
  "devDependencies": {
    "@types/webdriverio": "^4.10.1",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.6",
    "cucumber": "^4.2.1",
    "eslint": "^5.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "gherkin-lint": "^2.11.1",
    "mkdirp": "^0.5.1",
    "eslint-config-standard": "11.0.0-beta.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "should": "13.1.0",
    "wdio-junit-reporter": "0.3.1",
    "wdio-junit-to-html": "3.1.2",
    "wdio-cucumber-framework": "^1.1.1",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.12.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
